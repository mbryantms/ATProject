{
  "name": "atproject",
  "version": "1.0.0",
  "description": "A personal place for all my thoughts",
  "scripts": {
    "build": "npm-run-all --parallel build:css build:js:*",
    "build:css": "postcss static/css/src/*.css --dir static/css/dist/",
    "build:js": "npm-run-all --parallel build:js:*",
    "build:js:standalone": "esbuild static/js/image-focus.js static/js/sidenotes-standalone.js static/js/marginnotes-standalone.js static/js/fullwidth-standalone.js static/js/tablesorter-standalone.js --bundle --minify --target=es2017 --outdir=static/js/dist --legal-comments=none",
    "build:js:main": "esbuild static/js/main-entry.js --bundle --minify --target=es2017 --outfile=static/js/dist/main.js --legal-comments=none",
    "build:js:math": "esbuild static/js/math-copy.js --bundle --minify --target=es2017 --outfile=static/js/dist/math-copy.js --legal-comments=none",
    "dev": "npm-run-all --parallel watch:css watch:js:*",
    "watch:css": "postcss static/css/src/*.css --dir static/css/dist/ --watch",
    "watch:js:standalone": "npm run build:js:standalone -- --watch",
    "watch:js:main": "npm run build:js:main -- --watch",
    "watch:js:math": "npm run build:js:math -- --watch",
    "format": "prettier --write \"**/*.{css,html,js,json}\"",
    "format:check": "prettier --check \"**/*.{css,html,js,json}\"",
    "lint:js": "eslint \"**/*.js\" --fix"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "autoprefixer": "^10.4.21",
    "baseline-browser-mapping": "^2.9.6",
    "cssnano": "^7.1.1",
    "esbuild": "^0.25.10",
    "eslint": "^9.36.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "postcss-custom-properties": "^14.0.6",
    "postcss-import": "^16.1.1",
    "postcss-mixins": "^11.0.3",
    "postcss-nested": "^7.0.2",
    "postcss-sorting": "^9.1.0",
    "prettier": "^3.6.2"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.7.4"
  }
}
